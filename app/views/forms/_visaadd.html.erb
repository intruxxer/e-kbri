				<fieldset>
					<div class="row">	
						<section class="col col-6">
							<label class="label">Supporting Document</label>
							<label for="file" class="input input-file">
								<div class="button">
									<%= f.input_field :supdoc, as: :file, class: 'fileplaceholder' %>Upload</div>
									<input type="text" readonly="readonly" />
							</label>
							<div class="note">
								<strong>Note:</strong> 
								It could be company reference letter, sponsor letter, school admission letter, etc. 
								Combine all documents into one.
								Max Size: 2MB in ZIP/RAR or JPEG/JPG/PNG.</div>
						</section>
						
						<section class="col col-6">
							<label class="label">Return Trip Ticket</label>
							<label for="file" class="input input-file">
								<div class="button">
									<%= f.input_field :ticket, as: :file, class: 'fileplaceholder' %>Upload</div>
									<input type="text" readonly="readonly" />
							</label>
							<div class="note"><strong>Note:</strong> Format : JPG/JPEG/PNG/PDF. Max Size: 2MB</div>
						</section>	
					</div>	
				</fieldset>
				
				<fieldset class="newVisa specialFieldType no-show">
					<section><p style="text-align: right; font-weight: bold;">Seoul, <%= Date.current.strftime("%A, %v") %></p></section>
				</fieldset>
				
				<fieldset id="transit" class="specialFieldCat no-show">
				<div class="row">
				  <section class="col col-3">
					<label class="label">Country of Destination</label>
					<label class="input">
						<i class="icon-append icon-globe"></i>
						<%= f.input_field :count_dest %>
					</label>
					<div class="note"><strong>Note:</strong> Only fill this if you apply for TRANSIT VISA.</div>
				  </section>
				  
				  <section class="col col-2">
				    <label class="label">Flight/Vessel Name</label>
					<label class="input">
						<i class="icon-append icon-plane"></i>
						<%= f.input_field :flight_vessel %>
					</label>
				  </section>
				  
				  
				  
				  <section class="col col-3">
					<label class="label">Date of Entry to Indonesia</label>
					<label class="input">
						<i class="icon-append icon-calendar"></i>
						<%= f.input_field :date_entry, class: 'datePick' %>
					</label>
					<div class="note"><strong>Note:</strong> Format is DD/MM/YYYY. Example: 02/03/2014 is 02 March 2014.</div>
				  </section>
				</div>  
				</fieldset>

				<fieldset id="limited-stay" class="specialFieldCat">

			      <div class="row">
			        <section class="col col-4">
				      <label class="label">Immigration Approval No.</label>
					  <label class="input">
						<i class="icon-append icon-book"></i>
						<%= f.input_field :approval_no %>
					  </label>
					  <div class="note"><strong>Note:</strong> Input this Immigration Approval No. if you apply for Limited Stay Visa.</div>
				    </section>

				    <section class="col col-3">
					  <label class="label">Purpose of Application</label>
					  <label class="select">
						<%= f.input_field :purpose, 
						collection: { 'Visit' => 'visit', 'Work' => 'work', 'Joint Family' => 'family', 'Study' => 'study',
								      'Social' => 'social', 'Tourism' => 'tourism', 'Commercial' => 'commercial',
								      'Convention' => 'convention', 'Arts' => 'art', 'Family Visit' => 'family-visit',
								      'Sports' => 'sport', 'Posting' => 'posting', 'Training' => 'training', 
								      'Others' => 'other'} %>
							<i></i>
					  </label>
					  <div class="note">Please bring supporting <strong>Documents</strong> (if any) during interview/visa pick-up session.</div>
				    </section>
				    
				    
				  </div>
				</fieldset>

				<fieldset>
					<section class="col col-6">
						<div class="note">Please rewrite below code</div>					
						<label class="input">
							  <%= show_simple_captcha %>
						</label>
					</section>
				</fieldset>
				<fieldset class="newVisa specialFieldType">

					<section class="col col-9">
					I hereby declare that statements given above are true and I understand that even if granted a visa, 
					the admission at the airport remains fully the discretion of the immigration authorities in Indonesia.
					</section>
					
					<section class="col col-3">
					<pre>(Signature & Full Name)</pre>
					</section>				
				</fieldset>

				<footer>
					<% if controller_name == "visafamily" or controller_name == "visagroup"  %>
						<%= f.input_field :ref_id, type: 'hidden', value: session[:current_ref_id] %>	
					<% end %>
				
					<% if controller_name == "visafamily" or controller_name == "visagroup"  %>
						<button type="submit" class="button">Create & Add More Applicant(s)</button>
					<% else %>
						<%= link_to 'Go to User Panel', root_path %>
					<% end %>
					<button type="button" class="button button-secondary" onclick="window.history.back();">Back</button>
				</footer>
			<% end %>
