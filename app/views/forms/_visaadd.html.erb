<!-- Red color scheme -->
	<% if controller_name == "visafamily"  %>
		<% @url_response = visafamilys_path %>
	<% elsif controller_name == "visagroup"  %>
		<% @url_response = visagroups_path %>
	<% end %>
	<% unless @errors.nil? %>
			<h2 style="color:white;background:red;">Submission failed. The application is incomplete for:</h2>
				<div style="padding:4px;color:red;font-size:10pt;">
					<% @errors.each do |fs| %>
						<%= simple_format(fs[0].to_s + " = " + fs[1].to_s + "\n")  %>
					<%end%>
					<br />
					Silahkan Upload File - file anda kembali.
				</div>
	<%end%>
	<%= simple_form_for @visa, url: @url_response, :method => :post, :html => { :class => "sky-form", :multipart => true } do |f| %>
			<header>
				<% if controller_name == "visafamily" then %>
					Additional Applicants' Visa Form as Family
				<% elsif controller_name == "visagroup" %>
					Additional Applicants' Visa Form as Group
				<% else %>
				    Indonesian Visa Application Form for Individual
				<% end %>
			</header>
				<fieldset>
					<div class="row">
						<section class="col col-4">
							<label class="label">Finish Application</label>
							  <%= link_to finishgroupapply_path do %>
							  <span class="glyphicon-glyphicon-adjust" style="font-size:1.5em;">
							    <span class="glyphicon glyphicon-home"></span>
							    Finish
							  </span>													
						      <% end %>
						      <span style="font-size:1.5em; margin-left: 0.2em;">or add more applicant</span>
						</section>
					</div>
				</fieldset>
				
				<fieldset>
					<div class="row">
						<section class="col col-3">
							<label class="label">Application Type</label>
							<label class="select">
							<%= f.input_field :application_type, 
							collection: { 'New Visa Application' => 3, 'Extension of Re-Entry Permit' => 4}  %>
							<i></i>
							</label>
						</section>
						
						<section class="col col-3 isnotreentry">

							<label class="label">Requested Visa</label>
							<label class="select">
							<%= f.input_field :category_type, 
							collection: { 'Transit Visa' => 'transit', 'Visit Visa' => 'visit', 'Multiple Visa' => 'multiple', 'Visit Visa (with Immigration Approval)' => 'dirjenim',
								          'Limited Stay Visa' => 'limited-stay', 'Diplomatic Visa' => 'diplomatic', 
								          'Official Visa' => 'official' } %>
							<i></i>
							</label>
						</section>
						<section class="col col-2" style="display:none;">
							<label class="label">Type</label>
							<label class="select">
							<%= f.input_field :num_entry, :readonly => true, :default => 'Single' %>
							<i></i>
							</label>
							<div class="note">The decision is <strong>Subject</strong> to The Embassy of Indonesia's discretion.</div>
						</section>			
				
						<section class="col col-4 isnotreentry">
							<label class="label">Reason of Application (in Detail)</label>
							<label class="textarea textarea-expandable">
								<i class="icon-append icon-question"></i>
								<%= f.input_field :reason, as: :text, input_html: { row: 5 } %>
							</label>
							<div class="note"><strong>Please state your reason of visit.</strong> Example : Work, Training of Company X</div>
						</section>
					</div>
					<div class="row isnotreentry">
						<section class="col col-6"></section>
						<section class="col col-2">
							<label class="label">Duration of Stays</label>	
							<label class="input">								
								<%= f.input_field :duration_stays, :size => 3 %>
							</label>
						</section>
						<section class="col col-2">
								<label class="label">In</label>	
								<label class="select">
									<i class="icon-append icon-calendar"></i>
									<%= f.input_field :duration_stays_unit, 
									collection: { 'Days' => 'Day(s)', 'Months' => 'Month(s)', 'Years' => 'Month(s)' } %>
									<i></i>
								</label>
						</section>	
					</div>
				</fieldset>
				
				<fieldset class="newVisa specialFieldType no-show">
					<h3>Personal Detail</h3>
					<hr /><br />
					<div class="row">
						<section class="col col-4">
