<script type="text/javascript">
	$(function()
		{				
				$('input[type=text]').keyup(function() {
				    $(this).val($(this).val().toUpperCase());
				});
				$('textarea').keyup(function() {
				    $(this).val($(this).val().toUpperCase());
				});		
				
				<% if @visa.category_type != 'transit' %>
					$('#transit').hide();
				<% end %>
				<% if @visa.category_type != 'limited-stay' %>
					$('#limited-stay').hide();
				<% end %>
				<% if @visa.category_type != 'dirjenim'  %>
					$('#limited-stay').hide();
				<% end %>
				<% if @visa.category_type != 'multiple'  %>
					$('#limited-stay').hide();
				<% end %>
				<% if @visa.application_type != 3 %>
					$('.isnotreentry').hide();
				<% end %>
				
				$('#visa_application_type').change(function(){
					obj = $(this);
					if(obj.val()==3){						
						$('.isnotreentry').show();
					}else{
						$('.isnotreentry').hide();
					}
				});
				
				$('#visa_category_type').change(function(){
					obj = $(this);
					if(obj.val()=='transit'){
						$('#transit').show();
						$('#limited-stay').hide();
					}else if((obj.val()=='limited-stay')||(obj.val()=='dirjenim')||(obj.val()=='multiple')){
						$('#transit').hide();
						$('#limited-stay').show();
						$("#visa_num_entry").val("M");
					}else{
						$('#transit').hide();
						$('#limited-stay').hide();
					}
				});							
