<%= stylesheet_link_tag "/assets/css/dashboard/form.css" %>
<%= simple_form_for @visafee, url: '/passport/tospri/', :method => :post, :html => { :class => "sky-form", :multipart => true } do |f| %>				
				<fieldset>					
					<div class="row">
						<section class="col col-6">
							<label class="label">Jenis Visa</label>
							<label class="input">
								<%= @visafee.name_of_visa %>							
							</label>
						</section>
						<section class="col col-6">
							<label class="label">Biaya</label>
							<label class="input">
								<%= f.input_field :fee_of_visa %>							
							</label>
						</section>
					</div>					
				</fieldset>				
				<footer>
					<button type="submit" class="button">Simpan</button>
					<button type="button" class="button button-secondary" onclick="window.close();">Kembali</button>
				</footer>
<% end %>
